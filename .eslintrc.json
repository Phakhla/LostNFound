{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["app/assets/builds/*"],
  "globals": {
    "FontAwesome": "writable",
    "google": "writable"
  },
  "rules": {
    "class-methods-use-this": "off"
  }
}
